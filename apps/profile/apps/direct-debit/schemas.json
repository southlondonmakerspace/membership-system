{
	"cancelSubscriptionSchema": {
		"body": {
			"type": "object",
			"required": ["reason"],
			"properties": {
				"reason": {
					"type": "string",
					"minLength": 1
				}
			}
		}
	},
	"updateSubscriptionSchema": {
		"body": {
			"type": "object",
			"required": ["amount"],
			"properties": {
				"amount": {
					"type": "integer",
					"minimum": 1
				}
			}
		}
	}
}

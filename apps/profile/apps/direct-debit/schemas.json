{
	"cancelSubscriptionSchema": {
		"body": {
			"type": "object",
			"required": ["satisfied", "reason"],
			"properties": {
				"satisfied": {
					"type": "integer",
					"minimum": 1,
					"maximum": 5
				},
				"reason": {
					"type": "string",
					"minLength": 1
				},
				"other": {
					"type": "string"
				}
			}
		}
	},
	"updateSubscriptionSchema": {
		"body": {
			"type": "object",
			"required": ["amount"],
			"properties": {
				"amount": {
					"type": "integer",
					"minimum": 1
				}
			}
		}
	}
}
